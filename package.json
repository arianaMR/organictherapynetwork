    "jest-styled-components": "^6.3.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.19.1",
    "razzle-plugin-scss": "^3.0.0",
    "razzle-plugin-typescript": "^3.0.0",
    "source-map-explorer": "^2.0.1",
    "stylelint": "^9.6.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "typescript-plugin-styled-components": "^1.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:hook"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "stylelint",
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix",
      "stylelint",
      "prettier --write"
    ],
    "*.tsx": [
      "eslint --fix",
      "stylelint",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js",
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/razzle/config/jest/babelTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^react-spring(.*)$": "<rootDir>/test/__mocks__/reactSpringMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "modulePaths": [
      "<rootDir>/test"
    ],
    "setupFiles": [
      "<rootDir>/test/setupTests.js"
    ]
  }
}
