    "prettier": "npx prettier --write '**/*.js'",
    "test:cy": "cypress run",
    "open:cy": "cypress open -b chrome",
    "analyze": "source-map-explorer build/static/js/main.*",
    "introspection-query": "node ./tools/getFragmentTypes.js"
